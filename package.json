{
  "name": "forefront-parkeringsbokning-spa",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:emulator": "concurrently \"npm run emulators\" \"npm run dev\"",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:export ./firebase-export",
    "emulators:import": "firebase emulators:start --import ./firebase-export",
    "type-check": "tsc --noEmit",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "deploy:dev": "node scripts/deploy.js development hosting",
    "deploy:prod": "node scripts/deploy.js production hosting",
    "deploy:dev:all": "node scripts/deploy.js development all",
    "deploy:prod:all": "node scripts/deploy.js production all"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "firebase": "^10.13.2",
    "lucide-vue-next": "^0.553.0",
    "vue": "^3.5.12",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.1.0",
    "dotenv": "^16.4.5",
    "firebase-tools": "^13.15.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
